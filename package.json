{
  "name": "community",
  "version": "1.0.0",
  "description": "A social support platform for underserved individuals",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss css/*.css -d dist/css",
    "build:js": "webpack --mode production",
    "test": "jest",
    "lint": "eslint js/**/*.js",
    "format": "prettier --write \"**/*.{js,css,html,json}\""
  },
  "keywords": [
    "community",
    "social-support",
    "resources",
    "jobs",
    "translation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "css-loader": "^7.1.2",
    "cssnano": "^6.0.1",
    "eslint": "^8.40.0",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.8.8",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
